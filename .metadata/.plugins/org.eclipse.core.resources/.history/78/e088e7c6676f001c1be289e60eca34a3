<%
	Connection conn = null; // 접속 객체
	PreparedStatement pstmt = null;	// 쿼리객체
	ResultSet rs = null;	// 커리결과 객체
	String query = "";	// SQL 문법
	try {
		Class.forName("com.mysql.jdbc.Driver"); // JDBC 드라이버 로드
		//out.print("드라이버 로드 성공");
		String url = "jdbc:mysql://localhost:3306/mysql";	// 접속url
		String user = "root";
		String passwd = "";
		conn = DriverManager.getConnection(url, user, passwd);	// 디비에 연결
		//out.print(conn);
		query = "select * from tblboard";
		
	} catch (Exception e) {
		out.print(e);
	}
%>	
	
